# Increase backlight
"xbacklight -inc 10"
    XF86MonBrightnessUp
    
# Decrease backlight
"xbacklight -dec 10"
    XF86MonBrightnessDown

# Increase volume
"volume +5"
    XF86AudioRaiseVolume

# Decrease volume
"volume -5"
    XF86AudioLowerVolume

# Mute volume
"volume toggle"
    XF86AudioMute

# Take screenshot
"mkdir -p $HOME/Pictures/Screenshots; import $HOME/Pictures/Screenshots/$(date +%s).jpg"
    Print
